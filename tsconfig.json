{
  "compilerOptions": {
    "types": ["vite/client"],
    "target": "ESNext",
    "module": "commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "vueCompilerOptions": {
    "target": 3.3,
    "extensions": [".vue"]
  },
  "include": ["src"]
}
